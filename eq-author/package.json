{
  "name": "eq-author",
  "version": "0.0.1",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "yarn && node scripts/start.js",
    "build": "yarn build:status && node scripts/build.js",
    "build:status": "node scripts/build-status-page.js",
    "test": "node scripts/test.js",
    "test:storybook": "jest --testRegex ^.+\\.storytest\\.js$",
    "test:integration": "./scripts/run_tests.sh integration",
    "test:e2e": "./scripts/run_tests.sh e2e",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint src cypress --ext .graphql --ext .js --max-warnings=0",
    "lint:css": "stylelint './src/**/*.js'",
    "serve": "serve -l 3000 build/",
    "deploy": "npm run build && gh-pages -d build",
    "storybook": "export NODE_PATH=src/ && start-storybook -p 9001",
    "storybook-build": "export NODE_PATH=src/ && build-storybook -s public -o storybook-static",
    "storybook-deploy": "yarn storybook-build && gh-pages -d .out",
    "precommit": "lint-staged",
    "coverage": "yarn test -- --coverage",
    "cypress:open": "CYPRESS_baseUrl=${CYPRESS_baseUrl:=http://localhost:3000/} cypress open",
    "cypress:open:e2e": "CYPRESS_baseUrl=${CYPRESS_baseUrl:=http://localhost:13000/} cypress open --config integrationFolder=cypress/e2e"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.0.0-alpha.24",
    "@storybook/addon-knobs": "^4.0.0-alpha.24",
    "@storybook/addon-options": "^4.0.0-alpha.24",
    "@storybook/addon-storyshots": "^4.0.0-alpha.24",
    "@storybook/react": "^4.0.0-alpha.24",
    "autoprefixer": "^7.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "babel-plugin-named-asset-import": "^0.2.2",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-styled-components": "^1.8.0",
    "bfj": "^6.1.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "2.4.1",
    "connect-history-api-fallback": "^1.3.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "1.0.0",
    "cypress": "^3.1.0",
    "detect-port": "^1.2.1",
    "dotenv": "^6.0.0",
    "dotenv-expand": "^4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.1",
    "eslint-config-eq-author": "^2.0.1",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "2.1.1",
    "eslint-plugin-graphql": "^1.5.0",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "gh-pages": "^1.0.0",
    "gzip-size": "^4.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "http-proxy-middleware": "^0.17.4",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.2.0",
    "jest-transform-graphql": "^2.1.0",
    "json-loader": "^0.5.7",
    "json-schema-faker": "0.4.3",
    "lint-staged": "^4.2.3",
    "mini-css-extract-plugin": "^0.4.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pnp-webpack-plugin": "^1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "^6.0.10",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.0",
    "react-dev-utils": "^6.0.4",
    "react-router-test-context": "^0.1.0",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "^16.5.2",
    "recursive-readdir": "^2.2.1",
    "redux-mock-store": "^1.3.0",
    "resolve": "^1.8.1",
    "sass-loader": "7.1.0",
    "strip-ansi": "^4.0.0",
    "style-loader": "0.23.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.0.0",
    "svgo": "^1.1.1",
    "svgo-loader": "^2.2.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "terser-webpack-plugin": "^1.1.0",
    "url-loader": "1.1.1",
    "wait-on": "^2.0.2",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.9",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "^3.6.2"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-client-preset": "^1.0.8",
    "apollo-link": "1.1.0",
    "apollo-link-error": "^1.1.0",
    "apollo-link-http": "^1.5.3",
    "connected-react-router": "^4.2.1",
    "downshift": "^2.0.20",
    "draft-convert": "^2.0.0",
    "draft-js": "^0.10.5",
    "draft-js-raw-content-state": "^1.2.5",
    "draftjs-filters": "^1.0.0",
    "draftjs-to-html": "^0.7.6",
    "enzyme-to-json": "^3.3.1",
    "eq-author-graphql-schema": "^0.40.0",
    "firebase": "^4.6.2",
    "firebaseui": "2.4.1",
    "graphql": "^0.13.0",
    "graphql-anywhere": "^4.1.3",
    "graphql-iso-date": "^3.3.0",
    "graphql-tag": "^2.10.0",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "normalizr": "^3.2.4",
    "polished": "^1.8.0",
    "raven-js": "^3.19.1",
    "react": "^16.5.2",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.5.2",
    "react-firebaseui": "^1.0.10",
    "react-hot-loader": "^4.3.4",
    "react-modal": "^3.6.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-textarea-autosize": "^5.1.0",
    "react-titled": "^1.0.0",
    "react-tooltip": "^3.4.0",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "serve": "^7.2.0",
    "styled-components": "^3.4.9",
    "synchronous-promise": "^2.0.3",
    "uncontrollable": "^4.1.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*story.js",
      "!src/graphql/*.js",
      "!src/tests/**/*.js"
    ],
    "setupFiles": [
      "<rootDir>/src/tests/setup/throwOnConsole.js",
      "<rootDir>/src/tests/setup/mockLocalStorage.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/tests/setup/JestTestSetup.js",
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.svg\\?inline$": "<rootDir>/config/jest/svgTransform.js"
    },
    "modulePaths": [
      "<rootDir>/src/"
    ]
  },
  "moduleRoots": [
    "./src"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
