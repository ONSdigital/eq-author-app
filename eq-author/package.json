{
  "name": "eq-author",
  "version": "0.0.1",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "yarn && yarn build:fragmentTypes && node scripts/start.js",
    "build": "yarn build:fragmentTypes && yarn build:status && node scripts/build.js",
    "build:status": "node scripts/build-status-page.js",
    "build:fragmentTypes": "node scripts/buildFragmentTypes.js src/apollo/fragmentTypes.json",
    "test": "node scripts/test.js",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "yarn build:fragmentTypes && eslint src --ext .graphql --ext .js --max-warnings=0",
    "lint:css": "stylelint './src/**/*.js'",
    "serve": "serve -l 3000 build/",
    "coverage": "yarn test -- --coverage"
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/plugin-proposal-class-properties": "latest",
    "@babel/plugin-proposal-object-rest-spread": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-react": "latest",
    "@hot-loader/react-dom": "latest",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "latest",
    "autoprefixer": "latest",
    "babel-eslint": "latest",
    "babel-jest": "latest",
    "babel-loader": "latest",
    "babel-plugin-add-react-displayname": "latest",
    "babel-plugin-named-asset-import": "latest",
    "babel-plugin-styled-components": "latest",
    "bfj": "latest",
    "case-sensitive-paths-webpack-plugin": "latest",
    "chalk": "latest",
    "css-loader": "latest",
    "dotenv": "latest",
    "dotenv-expand": "latest",
    "enzyme": "latest",
    "enzyme-adapter-react-16": "latest",
    "enzyme-to-json": "latest",
    "eslint": "latest",
    "eslint-config-eq-author": "latest",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "latest",
    "eslint-plugin-graphql": "^1.5.0",
    "file-loader": "latest",
    "fs-extra": "latest",
    "html-webpack-plugin": "^4.0.0-alpha.2",
    "jest": "latest",
    "jest-styled-components": "latest",
    "jest-transform-graphql": "latest",
    "mini-css-extract-plugin": "latest",
    "optimize-css-assets-webpack-plugin": "latest",
    "pnp-webpack-plugin": "latest",
    "postcss-flexbugs-fixes": "latest",
    "postcss-loader": "latest",
    "postcss-preset-env": "latest",
    "postcss-safe-parser": "latest",
    "prop-types": "latest",
    "react-dev-utils": "latest",
    "react-router-test-context": "latest",
    "react-svg-loader": "latest",
    "redux-mock-store": "latest",
    "sass-loader": "latest",
    "style-loader": "latest",
    "stylelint": "latest",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-config-styled-components": "latest",
    "stylelint-processor-styled-components": "latest",
    "svgo": "latest",
    "svgo-loader": "latest",
    "terser-webpack-plugin": "latest",
    "url-loader": "latest",
    "wait-for-expect": "latest",
    "webpack": "latest",
    "webpack-dev-server": "latest",
    "webpack-manifest-plugin": "latest"
  },
  "dependencies": {
    "@apollo/react-testing": "latest",
    "@sentry/browser": "latest",
    "apollo-cache-inmemory": "latest",
    "apollo-client": "latest",
    "apollo-link": "latest",
    "apollo-link-context": "latest",
    "apollo-link-error": "latest",
    "apollo-link-http": "latest",
    "apollo-link-ws": "latest",
    "downshift": "latest",
    "draft-convert": "latest",
    "draft-js": "latest",
    "draft-js-block-breakout-plugin": "latest",
    "draft-js-plugins-editor": "latest",
    "draft-js-raw-content-state": "latest",
    "draftjs-filters": "^1.0.0",
    "firebase": "latest",
    "firebaseui": "latest",
    "graphql": "latest",
    "graphql-anywhere": "latest",
    "graphql-tag": "latest",
    "graphql-tools": "latest",
    "jsonwebtoken": "latest",
    "lodash": "latest",
    "moment": "latest",
    "polished": "latest",
    "react": "latest",
    "react-apollo": "latest",
    "react-dom": "latest",
    "react-firebaseui": "latest",
    "react-gtm-module": "latest",
    "react-hot-loader": "latest",
    "react-modal": "latest",
    "react-redux": "^5.0.7",
    "react-router": "latest",
    "react-router-dom": "latest",
    "react-router-redux": "latest",
    "react-string-replace": "latest",
    "react-textarea-autosize": "latest",
    "react-titled": "latest",
    "react-tooltip": "latest",
    "react-transition-group": "latest",
    "redux": "^3.7.2",
    "redux-devtools-extension": "latest",
    "redux-thunk": "latest",
    "styled-components": "^3.4.9",
    "subscriptions-transport-ws": "latest",
    "synchronous-promise": "latest",
    "uncontrollable": "^4.1.0",
    "uuid": "latest"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/graphql/*.js",
      "!src/tests/**/*.js",
      "!src/index.js"
    ],
    "setupFiles": [
      "<rootDir>/src/tests/setup/throwOnConsole.js",
      "<rootDir>/src/tests/setup/mockLocalStorage.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setup/JestTestSetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.svg\\?inline$": "<rootDir>/config/jest/svgTransform.js"
    },
    "modulePaths": [
      "<rootDir>/src/"
    ]
  },
  "moduleRoots": [
    "./src"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:4000"
}
