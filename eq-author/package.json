{
  "name": "eq-author",
  "version": "0.0.1",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "yarn && yarn build:fragmentTypes && node scripts/start.js",
    "build": "yarn build:fragmentTypes && yarn build:status && node scripts/build.js",
    "build:status": "node scripts/build-status-page.js",
    "build:fragmentTypes": "node scripts/buildFragmentTypes.js src/apollo/fragmentTypes.json",
    "test": "node scripts/test.js",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "yarn build:fragmentTypes && eslint src --ext .graphql --ext .js --max-warnings=0",
    "lint:css": "stylelint './src/**/*.js'",
    "serve": "serve -l 3000 build/",
    "coverage": "yarn test --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ../design-system --docs"
  },
