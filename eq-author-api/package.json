{
  "name": "eq-author-api",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "start": "node app.js",
    "start:dev": "NODE_ENV=development nodemon --ignore 'data/' --inspect=0.0.0.0:5858",
    "lint": "eslint .",
    "test": "./scripts/test.sh schema/tests",
    "test:breakingChanges": "node scripts/checkForBreakingChanges.js",
    "knex": "knex",
    "dynamodb-admin": "AWS_ACCESS_KEY_ID=dummy AWS_SECRET_ACCESS_KEY=dummy DYNAMO_ENDPOINT=http://localhost:8050 dynamodb-admin"
  },
  "dependencies": {
    "apollo-server-express": "latest",
    "aws-sdk": "latest",
    "body-parser": "latest",
    "cheerio": "latest",
    "cors": "latest",
    "deep-map": "^1.5",
    "dotenv": "latest",
    "dynamodb-admin": "latest",
    "dynamoose": "latest",
    "express": "latest",
    "express-pino-logger": "latest",
    "graphql": "latest",
    "graphql-anywhere": "latest",
    "graphql-iso-date": "latest",
    "graphql-tools": "latest",
    "graphql-type-json": "latest",
    "helmet": "latest",
    "js-yaml": "latest",
    "json-stable-stringify": "latest",
    "json-web-key": "latest",
    "jsondiffpatch": "latest",
    "jsonwebtoken": "latest",
    "jsrsasign": "latest",
    "knex": "latest",
    "lodash": "latest",
    "node-jose": "latest",
    "pg": "latest",
    "pg-hstore": "latest",
    "pino-noir": "latest",
    "uuid": "latest",
    "wait-on": "latest"
  },
  "engines": {
    "node": ">=7.10.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage/",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.js",
      "!config/**/*",
      "!migrations/*",
      "!coverage/**/*",
      "!tests/**/*",
      "!scripts/*",
      "!middleware/import.js",
      "!app.js"
    ],
    "watchPathIgnorePatterns": [
      "data/"
    ]
  },
  "devDependencies": {
    "eslint": "latest",
    "eslint-config-eq-author": "latest",
    "jest": "latest",
    "nodemon": "latest"
  }
}
