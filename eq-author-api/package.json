{
  "name": "eq-author-api",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "start": "node app.js",
    "start:dev": "NODE_ENV=development nodemon --inspect=0.0.0.0:5858",
    "lint": "eslint .",
    "test": "./scripts/test.sh",
    "test:coverage": "yarn test --coverage",
    "test:breakingChanges": "node scripts/checkForBreakingChanges.js",
    "knex": "knex --knexfile config/knexfile.js --cwd ."
  },
  "dependencies": {
    "aws-sdk": "^2.363.0",
    "body-parser": "^1.17.2",
    "cheerio": "^1.0.0-rc.2",
    "cors": "^2.8.3",
    "dotenv": "^6.0.0",
    "express": "^4.15.3",
    "express-pino-logger": "^4.0.0",
    "graphql": "^14.0.2",
    "graphql-iso-date": "^3.3.0",
    "graphql-relay": "^0.5.2",
    "graphql-server-express": "^1.3.2",
    "graphql-tools": "^4.0.0",
    "graphql-type-json": "^0.2.1",
    "js-yaml": "^3.12.0",
    "json-web-key": "^0.4.0",
    "jsonwebtoken": "^8.4.0",
    "jsrsasign": "^8.0.12",
    "knex": "^0.16.1",
    "lodash": "^4.17.4",
    "node-jose": "^1.0.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "wait-for-postgres": "^1.5.3"
  },
  "engines": {
    "node": ">=7.10.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "**/*.js",
      "!config/**/*",
      "!migrations/*",
      "!coverage/**/*",
      "!tests/**/*",
      "!scripts/*",
      "!middleware/import.js",
      "!app.js"
    ]
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-eq-author": "^2.0.1",
    "jest": "^23.6.0",
    "lint-staged": "^7.0.5",
    "nodemon": "^1.11.0"
  }
}
