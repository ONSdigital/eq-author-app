{
  "name": "eq-author-api",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "start": "node app.js",
    "start:dev": "ENABLE_OPENTRACING=true nodemon --ignore 'data/ migrations/' --inspect=0.0.0.0:5858",
    "lint": "eslint .",
    "test": "./scripts/test.sh",
    "test:ci": "./scripts/test-ci.sh",
    "test:breakingChanges": "node scripts/checkForBreakingChanges.js",
    "dynamodb-admin": "AWS_ACCESS_KEY_ID=dummy AWS_SECRET_ACCESS_KEY=dummy DYNAMO_ENDPOINT=http://localhost:8050 dynamodb-admin",
    "create-migration": "node scripts/createMigration.js"
  },
  "dependencies": {
    "ajv": "8.6.2",
    "ajv-errors": "3.0.0",
    "ajv-keywords": "5.0.0",
    "apollo-opentracing": "2.1.0",
    "apollo-server-express": "2.25.2",
    "body-parser": "1.19.0",
    "chalk": "4.1.1",
    "cheerio": "1.0.0-rc.10",
    "cors": "2.8.5",
    "deep-map": "1.5.0",
    "dotenv": "10.0.0",
    "dynamoose": "1.11.1",
    "express": "4.17.1",
    "express-pino-logger": "6.0.0",
    "firebase-admin": "9.11.0",
    "google-auth-library": "9.0.0",
    "graphql": "14.7.0",
    "graphql-anywhere": "4.2.7",
    "graphql-iso-date": "3.6.1",
    "graphql-redis-subscriptions": "2.1.1",
    "graphql-tools": "5.0.0",
    "graphql-type-json": "0.3.1",
    "helmet": "4.6.0",
    "ioredis": "4.28.2",
    "jaeger-client": "3.18.1",
    "js-yaml": "4.1.0",
    "json-web-key": "0.4.0",
    "jsondiffpatch": "0.4.1",
    "jsonwebtoken": "8.5.1",
    "jsrsasign": "10.3.0",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "mongodb": "3.7.3",
    "node-fetch": "2.6.1",
    "node-jose": "2.0.0",
    "pino-noir": "2.2.1",
    "pino-pretty": "5.1.0",
    "prom-client": "13.1.0",
    "request": "2.88.2",
    "uuid": "8.3.2",
    "wait-on": "6.0.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": false,
    "coverageReporters": [
      "json",
      "json-summary",
      "text",
      "text-summary",
      "lcov",
      "clover"
    ],
    "collectCoverageFrom": [
      "**/*.js",
      "!config/**/*",
      "!coverage/**/*",
      "!tests/**/*",
      "!**/tests/**/*",
      "!scripts/*",
      "!middleware/import.js",
      "!app.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/scripts/testSetup.js"
    ],
    "preset": "@shelf/jest-mongodb",
    "watchPathIgnorePatterns": [
      "data/"
    ]
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "2.0.1",
    "commander": "8.3.0",
    "dynamodb-admin": "4.1.0",
    "eslint": "7.31.0",
    "eslint-config-eq-author": "2.0.2",
    "jest": "26.6.3",
    "nodemon": "2.0.12",
    "supertest": "6.1.6"
  }
}
